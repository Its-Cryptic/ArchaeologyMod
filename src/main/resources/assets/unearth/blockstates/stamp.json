{
    "multipart": [
        {
            "when": { "facing": "north" },
            "apply": {
                "model": "unearth:block/stamp",
                "uvlock": true
            }
        },
        {
            "when": { "facing": "east" },
            "apply": {
                "model": "unearth:block/stamp",
                "y": 90,
                "uvlock": true
            }
        },
        {
            "when": { "facing": "south" },
            "apply": {
                "model": "unearth:block/stamp",
                "y": 180,
                "uvlock": true
            }
        },
        {
            "when": { "facing": "west" },
            "apply": {
                "model": "unearth:block/stamp",
                "y": 270,
                "uvlock": true
            }
        },
        {
            "when": { "facing": "up" },
            "apply": {
                "model": "unearth:block/stamp",
                "x": -90,
                "uvlock": true
            }
        },
        {
            "when": { "facing": "down" },
            "apply": {
                "model": "unearth:block/stamp",
                "x": 90,
                "uvlock": true
            }
        }
    ]
}
